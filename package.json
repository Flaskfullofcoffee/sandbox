{
  "name": "sandbox",
  "version": "1.0.0",
  "description": "a sandbox for familiarization with ES6, React, & Node.js",
  "main": "index.js",
  "scripts": {
    "sass:watch": "sass --watch scss:css",
    "build-watch": "webpack -d --watch",
    "server": "browser-sync start --server --files='dist/bundle.js,index.html' --no-notify css index.html scripts",
    "start": "concurrently 'npm run build-watch' 'npm run sass:watch' 'npm run server'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Flaskfullofcoffee/sandbox.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Flaskfullofcoffee/sandbox/issues"
  },
  "homepage": "https://github.com/Flaskfullofcoffee/sandbox#readme",
  "devDependencies": {
    "babel-core": "^5.6.15",
    "babel-loader": "^5.2.2",
    "babel-runtime": "^5.6.15",
    "node-libs-browser": "^0.5.2",
    "browser-sync": "^2.26.3",
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "webpack": "^4.29.3"
  },
  "dependencies": {
    "lodash": "^3.10.0"
  }
}
